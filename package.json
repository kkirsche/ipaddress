{
  "name": "ipaddress",
  "version": "1.0.0",
  "description": "A typescript port of the Python ipaddress module",
  "keywords": [
    "ip address",
    "ip",
    "ipv4",
    "ipv6",
    "ipaddress"
  ],
  "homepage": "https://github.com/kkirsche/ipaddress/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/kkirsche/ipaddress/issues"
  },
  "repository": {
    "url": "https://github.com/kkirsche/ipaddress.git",
    "type": "git"
  },
  "author": {
    "name": "Kevin Kirsche",
    "email": "kev.kirsche@gmail.com"
  },
  "scripts": {
    "format": "prettier -w src",
    "lint": "eslint",
    "test": "jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "packageManager": "yarn@3.6.0"
}
